{
    "$schema": "http://schema.management.azure.com/schemas/2014-04-01-preview/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "name": {
            "type": "string"
        },
        "storageAccountName": {
            "type": "string"
        },
        "storageAccountContainer": {
            "type": "string"
        }
    },
    "variables": {
        "storageId": "[resourceId('Microsoft.Storage/storageAccounts', parameters('storageAccountName'))]",
        "storageVersion": "[providers('Microsoft.Storage', 'storageAccounts').apiVersions[0]]"
    },
    "resources": [
        {
            "apiVersion": "2015-08-01",
            "name": "[concat(parameters('name'), '/logs')]",
            "type": "Microsoft.Web/sites/config",
            "properties": {
                "httpLogs": {
                    "azureBlobStorage": {
                        "enabled": true,
                        "sasUrl": ".....",
                        "retentionInDays": 180
                    }
                }
            },
            "location": "[resourceGroup().location]"
        }
    ],
    "outputs": {
        "temp": {
            "type": "array",
            "value": "[listServiceSAS(variables('storageId'), variables('storageVersion'))]"
        }
    }
}
